{
  "hosting": {
    "public": "dist",
    "rewrites": [
        {
            "source": "/404.html",
            "destination": "/notfound.html"
        },
        {
            "source": "**",
            "destination": "/index.html"
        }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
